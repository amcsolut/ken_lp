  <!-- Fallback para bibliotecas que não carregaram -->
  <script>
    // Fallback se jQuery não carregou
        if (typeof jQuery === 'undefined') {
            console.warn('jQuery não carregou, carregando versão de fallback...');
            document.write('<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.0/jquery.min.js"><\/script>');
        }
        
        // Verificar se as bibliotecas carregaram após um delay
        setTimeout(function() {
            // Fallback para Typed.js
            if (typeof Typed === 'undefined') {
                console.warn('Typed.js não carregou, aplicando fallback...');
                const typingElement = document.getElementById('typing');
                if (typingElement) {
                    typingElement.textContent = 'KEN';
                    typingElement.style.color = '#a98f75';
                }
            }
            
            // Fallback para AOS
            if (typeof AOS === 'undefined') {
                console.warn('AOS não carregou, removendo atributos data-aos...');
                const aosElements = document.querySelectorAll('[data-aos]');
                aosElements.forEach(function(el) {
                    el.removeAttribute('data-aos');
                    el.removeAttribute('data-aos-delay');
                    el.style.opacity = '1';
                    el.style.transform = 'none';
                });
            }
        }, 2000);
        
        // Fallback básico para smooth scroll sem jQuery
        if (typeof jQuery === 'undefined') {
            document.addEventListener('DOMContentLoaded', function() {
                const links = document.querySelectorAll('a[href^="#"]');
                links.forEach(function(link) {
                    link.addEventListener('click', function(e) {
                        e.preventDefault();
                        const target = document.querySelector(this.getAttribute('href'));
                        if (target) {
                            target.scrollIntoView({ behavior: 'smooth', block: 'start' });
                        }
                    });
                });
            });
        }
  </script>
